{
  "name": "@travi/language-scaffolder-prompts",
  "description": "shared Inquirer prompt definitions for language scaffolders used as part of the project scaffolder",
  "license": "MIT",
  "version": "0.0.0-semantically-released",
  "main": "lib/index.cjs.js",
  "module": "lib/index.es.js",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "lib/"
  ],
  "repository": "travi/language-scaffolder-prompts",
  "bugs": "https://github.com/travi/language-scaffolder-prompts/issues",
  "homepage": "https://npm.im/@travi/language-scaffolder-prompts",
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org/)",
  "scripts": {
    "test": "npm-run-all --print-label --parallel lint:* --parallel test:*",
    "lint:sensitive": "ban",
    "test:unit": "cross-env NODE_ENV=test c8 run-s test:unit:base",
    "test:unit:base": "DEBUG=any mocha --recursive test/unit",
    "lint:js": "eslint . --cache",
    "lint:md": "remark . --frail",
    "clean": "rimraf lib/",
    "build": "run-s clean build:*",
    "build:js": "rollup -c",
    "watch": "run-s 'build:js -- --watch'",
    "prepack": "run-s build",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@babel/register": "7.18.9",
    "@travi/any": "2.1.6",
    "@travi/babel-preset": "3.0.96",
    "@travi/eslint-config": "3.0.12",
    "@travi/eslint-config-mocha": "1.0.19",
    "ban-sensitive-files": "1.9.18",
    "c8": "7.12.0",
    "chai": "4.3.7",
    "commitlint-config-travi": "1.4.19",
    "cross-env": "7.0.3",
    "cz-conventional-changelog": "3.3.0",
    "husky": "8.0.2",
    "inquirer": "8.2.5",
    "mocha": "10.1.0",
    "npm-run-all": "4.1.5",
    "remark-cli": "11.0.0",
    "remark-preset-lint-travi": "4.0.2",
    "rimraf": "3.0.2",
    "rollup": "3.3.0",
    "rollup-plugin-auto-external": "2.0.0",
    "sinon": "14.0.2"
  },
  "peerDependencies": {
    "inquirer": "^6.0.0 || ^7.0.0 || ^8.0.0"
  }
}
