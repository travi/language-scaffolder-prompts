{
  "name": "@travi/language-scaffolder-prompts",
  "description": "shared Inquirer prompt definitions for language scaffolders used as part of the project scaffolder",
  "license": "MIT",
  "version": "0.0.0-semantically-released",
  "main": "lib/index.cjs.js",
  "module": "lib/index.es.js",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "lib/"
  ],
  "repository": "travi/language-scaffolder-prompts",
  "bugs": "https://github.com/travi/language-scaffolder-prompts/issues",
  "homepage": "https://npm.im/@travi/language-scaffolder-prompts",
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org/)",
  "scripts": {
    "test": "npm-run-all --print-label --parallel lint:* --parallel test:*",
    "lint:sensitive": "ban",
    "test:unit": "nyc run-s test:unit:base",
    "test:unit:base": "DEBUG=any mocha --recursive test/unit",
    "coverage:report": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "lint:js": "eslint . --cache",
    "lint:md": "remark . --frail",
    "lint:travis": "travis-lint .travis.yml",
    "clean": "rimraf lib/",
    "build": "run-s clean build:*",
    "build:js": "rollup -c",
    "watch": "run-s 'build:js -- --watch'",
    "prepack": "run-s build"
  },
  "devDependencies": {
    "@babel/register": "7.12.1",
    "@travi/any": "2.0.15",
    "@travi/babel-preset": "3.0.33",
    "@travi/eslint-config": "1.0.55",
    "@travi/eslint-config-mocha": "1.0.8",
    "@travi/travis-lint": "1.0.1",
    "ban-sensitive-files": "1.9.12",
    "chai": "4.2.0",
    "codecov": "3.8.1",
    "commitlint-config-travi": "1.3.11",
    "cz-conventional-changelog": "3.3.0",
    "husky": "4.3.0",
    "inquirer": "7.3.3",
    "mocha": "8.2.0",
    "npm-run-all": "4.1.5",
    "nyc": "15.1.0",
    "remark-cli": "9.0.0",
    "remark-preset-lint-travi": "1.3.11",
    "rimraf": "3.0.2",
    "rollup": "2.33.3",
    "rollup-plugin-auto-external": "2.0.0",
    "sinon": "9.2.1"
  },
  "peerDependencies": {
    "inquirer": "^6.0.0 || ^7.0.0"
  }
}
