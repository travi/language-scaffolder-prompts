{
  "name": "@travi/language-scaffolder-prompts",
  "description": "shared Inquirer prompt definitions for language scaffolders used as part of the project scaffolder",
  "license": "MIT",
  "version": "0.0.0-semantically-released",
  "main": "lib/index.cjs.js",
  "module": "lib/index.es.js",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "lib/"
  ],
  "repository": "travi/language-scaffolder-prompts",
  "bugs": "https://github.com/travi/language-scaffolder-prompts/issues",
  "homepage": "https://npm.im/@travi/language-scaffolder-prompts",
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org/)",
  "scripts": {
    "test": "npm-run-all --print-label --parallel lint:* --parallel test:*",
    "lint:sensitive": "ban",
    "test:unit": "nyc run-s test:unit:base",
    "test:unit:base": "DEBUG=any mocha --recursive test/unit",
    "coverage:report": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "lint:js": "eslint . --cache",
    "lint:md": "remark . --frail",
    "lint:travis": "travis-lint .travis.yml",
    "clean": "rimraf lib/",
    "build": "run-s clean build:*",
    "build:js": "rollup -c",
    "watch": "run-s 'build:js -- --watch'",
    "prepack": "run-s build"
  },
  "devDependencies": {
    "@babel/register": "^7.4.0",
    "@travi/any": "^1.8.5",
    "@travi/babel-preset": "^3.0.0",
    "@travi/eslint-config-travi": "^1.8.0",
    "ban-sensitive-files": "^1.9.2",
    "chai": "^4.2.0",
    "codecov": "^3.2.0",
    "commitlint-config-travi": "^1.2.40",
    "cz-conventional-changelog": "^2.1.0",
    "husky": "^1.3.1",
    "mocha": "^6.0.2",
    "npm-run-all": "^4.1.5",
    "nyc": "^13.3.0",
    "remark-cli": "^6.0.1",
    "remark-preset-lint-travi": "^1.2.0",
    "rimraf": "^2.6.3",
    "rollup": "^1.7.0",
    "rollup-plugin-auto-external": "^2.0.0",
    "sinon": "^7.3.0",
    "travis-lint": "^1.0.0"
  }
}
